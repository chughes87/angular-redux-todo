{
  "extends": ["airbnb", "plugin:jasmine/recommended"],
  "env": {
    "browser": true,
    "node": true,
    "jasmine": true
  },
  "rules": {
    "no-console": "off",
    "comma-dangle": "off",
    "indent": ["error", 4],
    "no-param-reassign": [0],
    "no-use-before-define": "off",
    "object-curly-newline": "off",
    "global-require": "off",
    "no-underscore-dangle": "off"
  },
  "plugins": [
    "jasmine"
  ]
}
