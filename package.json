{
  "name": "trulia-challenge",
  "version": "1.0.0",
  "description": "A fullstack js app that helps landlords price their rentals",
  "main": "src/server/index.js",
  "scripts": {
    "start": "node src/server/index.js",
    "build": "webpack --mode production",
    "client": "webpack-dev-server --mode development --devtool inline-source-map --hot",
    "server": "nodemon src/server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "test-init": "./node_modules/jasmine/bin/jasmine.js init",
    "test": "nodemon --ext js --exec 'jasmine \"src/**/*.spec.js\"'",
    "fetest": "karma start"
  },
  "author": "Sandeep Raveesh",
  "license": "ISC",
  "dependencies": {
    "csvtojson": "^2.0.4",
    "express": "^4.16.3",
    "fs": "0.0.1-security",
    "prop-types": "^15.6.2",
    "react-dom": "^16.3.1",
    "react-redux": "^5.0.7",
    "d3": "^5.5.0",
    "react": "^16.4.1",
    "redux": "^4.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "concurrently": "^3.5.1",
    "css-loader": "^0.28.11",
    "eslint": "^5.0.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jasmine": "^2.10.1",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "expect": "^23.2.0",
    "html-webpack-plugin": "^3.2.0",
    "jasmine": "^3.1.0",
    "karma": "^2.0.4",
    "karma-cli": "^1.0.1",
    "karma-mocha": "^1.3.0",
    "karma-webpack": "^3.0.0",
    "nodemon": "^1.17.3",
    "style-loader": "^0.21.0",
    "webpack": "^4.14.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-server": "^3.1.3"
  }
}
